+++ VirtualBox-2.1.2_OSE/src/recompiler_new/exec.c	2009-03-14 15:33:13.804150888 +0100
@@ -2751,7 +2751,7 @@ static void notdirty_mem_writeb(void *op
     int dirty_flags;
 #if defined(VBOX)
     ram_addr = addr;
-#elif
+#else
     ram_addr = addr - (unsigned long)phys_ram_base;
 #endif
 #ifdef VBOX
